<server>
  <classloading useJarUrls="true" />
  <library id="EAR_NAMELib">
    <fileset dir="${server.config.dir}/apps/EAR_NAME.ear/WAR_FILE/WEB-INF/lib" includes="*.jar" />
  </library>
  <enterpriseApplication id="EAR_NAME.ear" location="EAR_NAME.ear" name="EAR_NAME">
    <classloader commonLibraryRef="customLoginLib" delegation="parentFirst" privateLibraryRef="EAR_NAMELib" />
    <application-bnd>
      <security-role name="mdbuser">
        <run-as password="{xor}LCYsLz4sLA==" userid="SYSTEM" />
      </security-role>
      <security-role name="AllAuthenticated">
        <special-subject type="ALL_AUTHENTICATED_USERS" />
      </security-role>
      <security-role name="everyone">
        <special-subject type="ALL_AUTHENTICATED_USERS" />
      </security-role>
      <security-role name="mbeancreator">
        <user name="superuser" />
      </security-role>
    </application-bnd>
  </enterpriseApplication>
</server>
